[alias]
    ci = commit
    br = branch
    ch = checkout
    st = status
    gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[color]
    diff = auto
    status = auto
    branch = auto
[include]
    path = ~/dotfiles/.gitconfig.local
